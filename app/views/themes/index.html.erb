<p id="notice"><%= notice %></p>

<h1>Listing Theme</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @themes.each do |theme| %>
      <tr>
        <td><%= theme.theme_code %></td>
        <td><%= theme.theme_name %></td>

       <td><%= link_to 'Show', theme %></td>

	</tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Theme', new_theme_path %>
